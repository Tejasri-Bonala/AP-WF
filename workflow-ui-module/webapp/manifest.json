{
    "_version": "1.26.0",
    "sap.app": {
        "id": "com.aperam.workflowuimodule",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.8.4",
            "toolsId": "5dc81811-febf-4cf2-af55-63f7a8eaf493"
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.82.2",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.aperam.workflowuimodule.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            },
            "stepsModel": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/status.json"
            },
            "Feed": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "./model/Feed.json"
           }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.aperam.workflowuimodule.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteproductApprovalForm",
                    "pattern": ":?query:",
                    "target": [
                        "TargetproductApprovalForm"
                    ]
                }
            ],
            "targets": {
                "TargetproductApprovalForm": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewName": "App"
                }
            }
        },
        "rootView": {
            "viewName": "com.aperam.workflowuimodule.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "productapproval"
    },
    "sap.bpa.task": {
        "_version": "1.0.0",
        "outcomes": [
            {
                "id": "approve",
                "label": "Approve"
            },
            {
                "id": "reject",
                "label": "Reject"
            }
        ],
        "inputs": {
            "$schema": "http://json-schema.org/draft-07/schema",
            "title": "input",
            "type": "object",
            "properties": {
                "projectNumber": {
                    "type": "string",
                    "title": "PROJECT_NUMBER",
                    "description": "PROJECT_NUMBER"
                },
                "cto_label": {
                    "type": "string",
                    "title": "CTO_LABEL",
                    "description": "CTO_LABEL"
                },
                "project_maturity": {
                    "type": "string",
                    "title": "PROJECT_MATURITY",
                    "description": "PROJECT_MATURITY"
                },
                "title": {
                    "type": "string",
                    "title": "TITLE",
                    "description": "TITLE"
                },
                "accounting_type": {
                    "type": "string",
                    "title": "ACCOUNTING_TYPE",
                    "description": "ACCOUNTING_TYPE"
                },
                "currency": {
                    "type": "string",
                    "title": "CURRENCY",
                    "description": "CURRENCY"
                },
                "forex": {
                    "type": "string",
                    "title": "FOREX",
                    "description": "FOREX"
                },
                "perimeter": {
                    "type": "string",
                    "title": "PERIMETER",
                    "description": "PERIMETER"
                },
                "plant": {
                    "type": "string",
                    "title": "PLANT",
                    "description": "PLANT"
                },
                "capex": {
                    "type": "string",
                    "title": "CAPEX",
                    "description": "CAPEX"
                },
                "opex": {
                    "type": "string",
                    "title": "OPEX",
                    "description": "OPEX"
                },
                "lease": {
                    "type": "string",
                    "title": "LEASE",
                    "description": "LEASE"
                },
                "categories": {
                    "type": "string",
                    "title": "CATEGORIES",
                    "description": "CATEGORIES"
                },
                "year": {
                    "type": "string",
                    "title": "YEAR",
                    "description": "YEAR"
                },
                "comments":{
                    "type": "object",
                    "title": "COMMENTS",
                    "description": "COMMENTS"
                }
            }
        },
        "outputs": {
            "$schema": "http://json-schema.org/draft-07/schema",
            "title": "output",
            "type": "object",
            "properties": {
                "projectNumber": {
                    "type": "string",
                    "title": "PROJECT_NUMBER",
                    "description": "PROJECT_NUMBER"
                },
                "cto_label": {
                    "type": "string",
                    "title": "CTO_LABEL",
                    "description": "CTO_LABEL"
                },
                "project_maturity": {
                    "type": "string",
                    "title": "PROJECT_MATURITY",
                    "description": "PROJECT_MATURITY"
                },
                "title": {
                    "type": "string",
                    "title": "TITLE",
                    "description": "TITLE"
                },
                "accounting_type": {
                    "type": "string",
                    "title": "ACCOUNTING_TYPE",
                    "description": "ACCOUNTING_TYPE"
                },
                "currency": {
                    "type": "string",
                    "title": "CURRENCY",
                    "description": "CURRENCY"
                },
                "forex": {
                    "type": "string",
                    "title": "FOREX",
                    "description": "FOREX"
                },
                "perimeter": {
                    "type": "string",
                    "title": "PERIMETER",
                    "description": "PERIMETER"
                },
                "plant": {
                    "type": "string",
                    "title": "PLANT",
                    "description": "PLANT"
                },
                "capex": {
                    "type": "string",
                    "title": "CAPEX",
                    "description": "CAPEX"
                },
                "opex": {
                    "type": "string",
                    "title": "OPEX",
                    "description": "OPEX"
                },
                "lease": {
                    "type": "string",
                    "title": "LEASE",
                    "description": "LEASE"
                },
                "categories": {
                    "type": "string",
                    "title": "CATEGORIES",
                    "description": "CATEGORIES"
                },
                "year": {
                    "type": "string",
                    "title": "YEAR_N",
                    "description": "YEAR_N"
                },
                "comments":{
                    "type": "string",
                    "title": "COMMENTS",
                    "description": "COMMENTS"
                }
            }
        },
        "category": "approval"
    }
}